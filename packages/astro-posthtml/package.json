{
  "name": "astro-posthtml",
  "version": "1.1.1",
  "description": "Transform Astro files with PostHTML",
  "homepage": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-posthtml",
  "repository": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-posthtml",
  "license": "Apache-2.0",
  "author": "Amin Yahyaabadi <aminyahyaabadi74@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "scripts": {
    "build": "shx rm -rf dist && tsc --pretty",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "posthtml": "^0.16.6"
  },
  "devDependencies": {},
  "peerDependencies": {
    "astro": "4.5.9"
  },
  "engines": {
    "node": ">=16.x"
  },
  "keywords": [
    "astro",
    "posthtml",
    "transform",
    "plugin",
    "astro-plugin",
    "astro-htmlnano",
    "astro-cssnano",
    "astro-minify",
    "astro-minifier",
    "astro-middleware",
    "optimize"
  ],
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": ">=16.x"
      },
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": false
    }
  }
}
