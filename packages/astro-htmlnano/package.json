{
  "name": "astro-htmlnano",
  "version": "1.4.4",
  "description": "Minify Astro files with HTMLNano and CSSNano",
  "homepage": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-htmlnano",
  "repository": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-htmlnano",
  "license": "Apache-2.0",
  "author": "Amin Yahyaabadi <aminyahyaabadi74@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "scripts": {
    "build": "shx rm -rf dist && tsc --pretty",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "astro-posthtml": "workspace:*",
    "cssnano": "^7.0.0",
    "htmlnano": "2.1.1",
    "postcss": "^8.4.38",
    "relateurl": "^0.2.7",
    "srcset": "^5.0.1",
    "svgo": "^3.2.0",
    "terser": "^5.29.2"
  },
  "devDependencies": {
    "@types/relateurl": "^0.2.33"
  },
  "peerDependencies": {
    "astro": ">=4"
  },
  "engines": {
    "node": ">=16.x"
  },
  "keywords": [
    "astro",
    "htmlnano",
    "cssnano",
    "minify",
    "astro-plugin",
    "astro-htmlnano",
    "astro-cssnano",
    "astro-minify",
    "astro-minifier",
    "astro-middleware",
    "optimize"
  ],
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": ">=16.x"
      },
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": false
    }
  }
}
