{
  "name": "astro-htmlnano",
  "version": "1.0.1",
  "description": "Minify Astro files with HTMLNano and CSSNano",
  "homepage": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-htmlnano",
  "repository": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-htmlnano",
  "license": "Apache-2.0",
  "author": "Amin Yahyaabadi <aminyahyaabadi74@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "scripts": {
    "build": "tsc --pretty"
  },
  "dependencies": {
    "astro-posthtml": "workspace:*",
    "cssnano": "^6.0.0",
    "htmlnano": "github:aminya/htmlnano#f00904018e031ff3246f2d000124e7d49206cfb1"
  },
  "peerDependencies": {
    "astro": "3.2.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "keywords": [
    "astro",
    "htmlnano",
    "cssnano",
    "minify",
    "astro-plugin",
    "astro-htmlnano",
    "astro-cssnano",
    "astro-minify",
    "astro-minifier",
    "astro-middleware",
    "optimize"
  ],
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": ">=16.x"
      },
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": false
    }
  }
}
