{
  "name": "astro-htmlnano",
  "version": "1.4.5",
  "description": "Minify Astro files with HTMLNano and CSSNano",
  "homepage": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-htmlnano",
  "repository": "https://github.com/aminya/astro-plugins/tree/main/packages/astro-htmlnano",
  "license": "Apache-2.0",
  "author": "Amin Yahyaabadi <aminyahyaabadi74@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "scripts": {
    "build": "shx rm -rf dist && tsc --pretty",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "astro-posthtml": "^1.1.3",
    "cssnano": "^7.0.6",
    "htmlnano": "^2.1.1",
    "postcss": "^8.5.3",
    "relateurl": "^0.2.7",
    "srcset": "^5.0.1",
    "svgo": "^3.3.2",
    "terser": "^5.39.0"
  },
  "devDependencies": {
    "@types/relateurl": "^0.2.33"
  },
  "peerDependencies": {
    "astro": "^4 || ^5"
  },
  "engines": {
    "node": ">=16.x"
  },
  "keywords": [
    "astro",
    "htmlnano",
    "cssnano",
    "minify",
    "astro-plugin",
    "astro-htmlnano",
    "astro-cssnano",
    "astro-minify",
    "astro-minifier",
    "astro-middleware",
    "optimize"
  ],
  "targets": {
    "main": {
      "context": "node",
      "engines": {
        "node": ">=16.x"
      },
      "isLibrary": true,
      "includeNodeModules": true,
      "optimize": false
    }
  }
}
